{
  "name": "@complete-cad/aps-viewer-filters",
  "version": "0.0.1-development",
  "description": "Smart filters for APS viewer",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && move lib\\esm\\index.js lib\\esm\\index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build",
    "grammar": "ohm generateBundles src/*.ohm --withTypes",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CADBIMDeveloper/APS-viewer-filters.git"
  },
  "keywords": [
    "Autodesk Platform Services viewer",
    "Forge viewer",
    "filter"
  ],
  "author": "Alex Ignatovich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CADBIMDeveloper/APS-viewer-filters/issues"
  },
  "homepage": "https://github.com/CADBIMDeveloper/APS-viewer-filters#readme",
  "devDependencies": {
    "@ohm-js/cli": "^1.1.0",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "ohm-js": "^16.6.0"
  }
}
